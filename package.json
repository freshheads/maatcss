        "sass-loader": "^10.0.2",
        "style-loader": "^1.2.1",
        "stylelint": "^13.7.1",
        "stylelint-bare-webpack-plugin": "^2.0.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
