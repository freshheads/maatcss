{
    "name": "@freshheads/maatcss",
    "description": "Extensible, scalable, Sass-based modules that can be used as starting point for UI projects",
    "version": "1.0.3",
    "engines": {
        "node": ">=12.13.0",
        "npm": ">=6.0.0"
    },
    "main": "example.main.scss",
    "scripts": {
        "test": "npm run lint",
        "lint": "./node_modules/.bin/stylelint \"**/*.scss\" --syntax scss",
